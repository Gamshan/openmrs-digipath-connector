<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.digipath.connector.proforma">
    <class name="org.openmrs.module.digipath.connector.proforma.DigipathConnector" table="digipath_connector">
        <id name="digipathConnectorId" type="int" column="digipath_connector_id" unsaved-value="0">
            <generator class="identity"/>
        </id>
        <discriminator column="digipath_connector_id" insert="false" />
        <property name="url" type="java.lang.String" column="url" length="255" not-null="true"/>
        <property name="username" type="java.lang.String" column="username" length="255"/>
        <property name="description" type="java.lang.String" column="description" length="255"/>
    </class>
</hibernate-mapping>